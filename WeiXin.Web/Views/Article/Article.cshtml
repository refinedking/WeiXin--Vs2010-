@model WeiXin.BO.GuanJianZiHuiFuContract
@{
    ViewBag.Title = Model.title;
}
@section css{
    

    <link href="http://weixin.cqzuxia.com/Styles/Enterprise/style.css" rel="stylesheet" type="text/css" />   
<style type="text/css">
 
.a_list {
	background: rgb(255, 255, 255); margin: 10px; border-radius: 5px; border: 1px solid rgb(221, 221, 221); font-family: "Microsoft YaHei";
}
.text img{ width:auto;height:auto;}
</style>}
<section class="i_list">
		 
<div class="page-bizinfo">
    <div class="header" id="header">
        <h1 id="activity-name">
            @Model.title</h1>
        <p class="activity-info"  >
            <span id="post-date" class="activity-meta no-extra" >更新时间： @DateTime.Parse(Model.time).ToString("yyyy-MM-dd") &nbsp; &nbsp; <span style="margin-right:50px;"> 阅读次数： @Model.hfCount 次</span></span></p>
    </div>
</div>
<div class="page-content">
    <div class="media" id="media">
    </div>
    <div class="text">   
        @Html.Raw(Model.content)
    </div>
</div>
 
</section>
<section class="i_list">
<ul>
  <li style="height:18px;text-align:center"><a href="#header">返回顶部</a>
</li>
</ul>
</section>
     <footer>   <p>&copy; 2013 重庆足下科技有限公司</p>   <p><a href="javascript:void(0)">关于我们</a><a href="javascript:void(0)">联系我们</a></p>  </footer>
