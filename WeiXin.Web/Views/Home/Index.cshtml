@{
    ViewBag.Title = "微联盟企业微信公共管理平台登录页面";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section css{
    <link href="@Url.Content("~/Styles/Index.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Styles/Zuxia2013.css")" rel="stylesheet" type="text/css" />
}@section script{
    <script src="@Url.Content("~/Scripts/Index.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/PlugIn/Zuxia2013.js")" type="text/javascript"></script>
}
<div id="divPage" class="disSelect">
    <div id="divTop">
        <div id="divLog">
        </div>
        <div id="divSysName">
        </div>
        <div id="divChangeSystem">
            [切换系统]</div>
        <div id="divSystemList">
            <ul>
                <!--enName:系统英文名 sysId:系统编号-->
                @Html.Raw(ViewBag.sysMenu)
            </ul>
        </div>
        <div id="divCtrl">
            欢迎您：<span id="spanUserName" title="点击进入个人资料页面" style="color: rgb(0, 94, 172); cursor: pointer;">@((Session["user"] as WeiXin.BO.UsersContract).wxName)</span> &nbsp;|&nbsp;<span>用户组：【@((Session["user"] as WeiXin.BO.UsersContract).positionName)】</span>&nbsp;|&nbsp;@Html.ActionLink("重登录", "LogOff", "Home", new { title = "系统发生的部分异常，可以使用重新登录来解决" })&nbsp;|&nbsp;
            @*<a href="javascript:window.opener=null;window.open('','_self');window.close(); "
                title="退出系统后，记得关闭浏览器">退出</a>*@ <a href="javascript:window.open('','_self');window.close(); " title="退出系统后，记得关闭浏览器">
                    退出</a>
        </div>
        <div id="divMenu">
        </div>
    </div>
    <!--分割线-->
    <div id="divPartingLine">
    </div>
    <!--主操作区域-->
    <div id="divMiddle">
        <!--tab样式-->
        <div id="divTab">
        </div>
        <!--主操作页面框架样式-->
        <div id="divTabItem">
           <iframe id="ifrmOtherFrame" src="@Url.Action("HomePage")"></iframe>
        </div>
    </div>
    <input type="hidden" id="hidIndexUrl" value="@Url.Action("HomePage", "Home")" />
    <input type="hidden" id="hidUserInfoUrl" value="@Url.Action("OperateEmployee", "WeiXin", new { Area = "WeiXin_Public", id = ((Session["user"] as WeiXin.BO.UsersContract).EId) })" />
     
</div>

