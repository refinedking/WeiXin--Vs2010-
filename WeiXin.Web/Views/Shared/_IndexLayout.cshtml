@{
    ViewBag.Title = "_IndexLayout";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section css
{
    <link href="@Url.Content("~/Styles/_IndexLayout.css")" rel="stylesheet" type="text/css" />
    @RenderSection("css", false)
}
@section script
{
    <script src="@Url.Content("~/Scripts/_IndexLayout.js")" type="text/javascript"></script>
    @RenderSection("script", false)
}
@RenderBody()
